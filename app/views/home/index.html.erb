<div id="main" role="main">

<article id="home">
  <div class="container">
    
    <!--FACEBOOK-->
    <div id="facebook_division" style="margin-bottom:5px;margin-left:5px;">
      <div class="fb-like" data-href="http://likelion.net" data-send="false" data-layout="button_count" data-width="450" data-show-faces="false"></div>
    </div>
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
